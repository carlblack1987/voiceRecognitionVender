                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Name="B_Disable" BorderThickness="0" Background="{DynamicResource Button disabled}"/>
                        <Border x:Name="B_Pressed" BorderThickness="0" Background="{DynamicResource Button down}"/>
                        <Border x:Name="B_Normal" BorderThickness="0" Background="{DynamicResource Button normal}"/>

                        <ContentPresenter RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,30">
                            <ContentPresenter.Effect>
                                <DropShadowEffect Opacity="0.3" ShadowDepth="2"/>
                            </ContentPresenter.Effect>
                            <ContentPresenter.ContextMenu>
                                <ContextMenu/>
                            </ContentPresenter.ContextMenu>
                        </ContentPresenter>
                        <Image Margin="0,45,0,0" Source="pack://siteoforigin:,,,/Images/ManagerPic/Pic_DeviceMenu/DeviceCfg_Menu_Cancel.png" Stretch="Fill" Height="103" Width="102" HorizontalAlignment="Center" VerticalAlignment="Top"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsFocused" Value="True"/>
                        <Trigger Property="IsDefaulted" Value="True"/>
                        <Trigger Property="IsMouseOver" Value="True"/>
                        <Trigger Property="IsPressed" Value="True"/>
                        <Trigger Property="IsEnabled" Value="False"/>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- *********************硬件设备维护菜单样式结束*********************-->
</ResourceDictionary>